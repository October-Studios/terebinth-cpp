set(TEREBINTH_VERSION_MAJOR @PROJECT_VERSION_MAJOR@)
set(TEREBINTH_VERSION_MINOR @PROJECT_VERSION_MINOR@)
set(TEREBINTH_VERSION_PATCH @PROJECT_VERSION_PATCH@)
set(TEREBINTH_VERSION @PROJECT_VERSION@)

@PACKAGE_INIT@

set_and_check(TEREBINTH_INCLUDE_DIR "@PACKAGE_CMAKE_INSTALL_INCLUDEDIR@")
set_and_check(TEREBINTH_LIB_DIR "@PACKAGE_CMAKE_INSTALL_LIBDIR@")
set_and_check(TEREBINTH_PACKAGE_DIR "@PACKAGE_CMAKE_INSTALL_LIBDIR/cmake/terebinth-@PROJECT_VERSION@")
if(${_NAME}_doc_FOUND)
	set_and_check(TEREBINTH_DOC_DIR "@PACKAGE_CMAKE_INSTALL_DOCDIR@")
endif()

include(${CMAKE_CURRENT_LIST_DIR}/terebinth-targets.cmake)

list(APPEND CMAKE_MODULE_PATH ${CMAKE_CURRENT_LIST_DIR}/Modules)
list(APPEND CMAKE_PROGRAM_PATH ${CMAKE_CURRENT_LIST_DIR}/Modules)

include(${CMAKE_CURRENT_LIST_DIR}/terebinth-api.cmake)
